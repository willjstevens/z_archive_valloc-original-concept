<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.valloc.server.ServerApplication">
         </run>
      </application>
   </extension>
   <extension
         id="valloc_server"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.valloc.server.application"
            name="Valloc Server">
         <property
               name="appName"
               value="Valloc Server">
         </property>
      </product>
   </extension>

</plugin>
